<plugin>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <actionProvider
            class="org.artop.ecuc.gautosar.codegen.xpand.ui.actions.providers.EcucCodeGenActionProvider"
            id="org.artop.ecuc.gautosar.codegen.xpand.ui.ecucCodeGenActionProvider">
         <enablement>
	        <instanceof value="gautosar.gecucdescription.GModuleConfiguration" />
         </enablement>
      </actionProvider>
      <actionProvider
            class="org.artop.ecuc.gautosar.codegen.xpand.ui.actions.providers.EcucAutoConfigureActionProvider"
            id="org.artop.ecuc.gautosar.codegen.xpand.ui.ecucAutoConfigureActionProvider">
         <enablement>
            <or>
               <instanceof
                     value="gautosar.gecucdescription.GModuleConfiguration">
               </instanceof>
               <instanceof
                     value="gautosar.gecucdescription.GContainer">
               </instanceof>
               <instanceof
                     value="gautosar.gecucdescription.GParameterValue">
               </instanceof>
               <instanceof
                     value="gautosar.gecucdescription.GConfigReferenceValue">
               </instanceof>
            </or>
         </enablement>
      </actionProvider>
   </extension>
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            class="org.artop.ecuc.gautosar.codegen.xpand.ui.decorators.EcucOutletFolderLabelDecorator"
            id="org.artop.ecuc.gautosar.codegen.xpand.ui.decorators.outletFolder"
            label="%ecuc.gautosar.codegen.xpand.ui.decorators.outletFolder.label"
            lightweight="true"
            state="true">
         <description>
            %ecuc.gautosar.codegen.xpand.ui.decorators.outletFolder.description
         </description>
      </decorator>
   </extension>
</plugin>
