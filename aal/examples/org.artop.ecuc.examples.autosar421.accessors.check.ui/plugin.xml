<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<extension point="org.eclipse.ui.navigator.viewer">
      <viewerActionBinding viewerId="org.eclipse.ui.navigator.ProjectExplorer">
         <includes>
            <actionExtension
                  pattern="org.artop.ecuc.examples.accessors.check.ui.validation">
            </actionExtension>
         </includes>
      </viewerActionBinding>
      <viewerActionBinding
            viewerId="org.artop.aal.examples.explorer.views.autosarExplorer">
         <includes>
            <actionExtension
                  pattern="org.artop.ecuc.examples.accessors.check.ui.validation">
            </actionExtension>
         </includes>
      </viewerActionBinding>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <actionProvider
            class="org.artop.ecuc.examples.autosar421.accessors.check.ui.actions.EcucValueAccessorCheckValidationActionProvider"
            id="org.artop.ecuc.examples.accessors.check.ui.validation">
         <enablement>
            <or>
               <instanceof
                     value="gautosar.gecucdescription.GModuleConfiguration">
               </instanceof>
			</or>
         </enablement>
      </actionProvider>
   </extension>
</plugin>
