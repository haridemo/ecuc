<plugin>

   <!-- Common navigator viewer action binding contribution for Hummingbird metamodel generation actions  -->

   <extension point="org.eclipse.ui.navigator.viewer">
      <viewerActionBinding viewerId="org.eclipse.ui.navigator.ProjectExplorer">
         <includes>
            <actionExtension pattern="org.artop.ecuc.examples.autosar4x.parametervalueaccessorgen.ui.actions" />
         </includes>
      </viewerActionBinding>
      <viewerActionBinding
            viewerId="org.artop.aal.examples.explorer.views.autosarExplorer">
         <includes>
            <actionExtension
                  pattern="org.artop.ecuc.examples.autosar4x.parametervalueaccessorgen.ui.actions">
            </actionExtension>
         </includes>
      </viewerActionBinding>
   </extension>

   <!-- Common navigator content contribution for Hummingbird metamodel generation actions -->

   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <actionProvider
            class="org.artop.ecuc.examples.autosar4x.accessorgen.ui.actions.providers.ParameterValueAccessorGenActionProvider"
            id="org.artop.ecuc.examples.autosar4x.parametervalueaccessorgen.ui.actions">
         <enablement>
            <or></or>
         </enablement>
      </actionProvider>
   </extension>

</plugin>
